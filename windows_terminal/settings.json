                "window",
                "taskbar"
            ],
            "closeOnExit": "graceful",
            "colorScheme": "Edge-Light",
            "cursorShape": "filledBox",
            "font": 
            {
                "face": "Iosevka",
                "size": 13
            },
            "opacity": 80,
            "scrollbarState": "hidden",
            "padding": "2, 2, 2, 2",
            "useAcrylic": true
        },
